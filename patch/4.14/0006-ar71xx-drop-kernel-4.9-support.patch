-	case 0:
-		oobregion->offset = 8;
-		oobregion->length = 3;
-		return 0;
-	case 1:
-		oobregion->offset = 13;
-		oobregion->length = 3;
-		return 0;
-	default:
-		return -ERANGE;
-	}
-}
-
-static int rbsxtlite_ooblayout_free(struct mtd_info *mtd, int section,
