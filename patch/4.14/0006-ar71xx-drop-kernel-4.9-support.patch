-	if (strstr(mips_get_machine_name(), "750UP r2"))
-		flags |= RBSPI_HAS_USB | RBSPI_HAS_POE;
-
-	/* differentiate the Powerbox from the hEX lite */
-	else if (strstr(mips_get_machine_name(), "750P r2"))
