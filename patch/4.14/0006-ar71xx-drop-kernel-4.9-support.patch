-	if (flags & RB2011_FLAG_SFP)
-		rb2011_sfp_init();
-
-	if (flags & RB2011_FLAG_WLAN)
-		rb2011_wlan_init();
-
-	if (flags & RB2011_FLAG_USB)
-		ath79_register_usb();
-
-	return 0;
