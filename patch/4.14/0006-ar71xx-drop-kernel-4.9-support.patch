-	/* GMAC1 is connected to the internal switch */
-	ath79_eth1_data.phy_if_mode = PHY_INTERFACE_MODE_GMII;
-	ath79_init_mac(ath79_eth1_data.mac_addr, mac, 2);
-	ath79_register_eth(1);
