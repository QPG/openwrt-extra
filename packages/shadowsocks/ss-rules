#global 
sudo ss-rules -s $server -l 1080 -d SS_SPEC_WAN_FW -o -I enp0s3

#chnroute
sudo ss-rules -s $server -l 1080 -B /home/john/chnroute.txt -d SS_SPEC_WAN_AC -o -I enp0s3

#gfwlist
sudo ss-rules -s $server -l 1080 -b 0.0.0.0/1 128.0.0.0/1 -d SS_SPEC_WAN_AC -o -I enp0s3
sudo ipset create gfwlist hash:ip
sudo iptables -t nat -I SS_SPEC_WAN_AC 1 -m set --match-set gfwlist dst -j SS_SPEC_WAN_FW
