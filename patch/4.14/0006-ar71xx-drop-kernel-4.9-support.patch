-		oobregion->offset = 0;
-		oobregion->length = 4;
-		return 0;
-	case 1:
-		oobregion->offset = 4;
-		oobregion->length = 1;
-		return 0;
-	case 2:
-		oobregion->offset = 6;
-		oobregion->length = 2;
-		return 0;
-	case 3:
-		oobregion->offset = 11;
-		oobregion->length = 2;
-		return 0;
