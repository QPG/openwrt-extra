-{
-	rbspi_gpio_keys_reset[0].gpio = gpio;
-	ath79_register_gpio_keys_polled(-1, RBSPI_KEYS_POLL_INTERVAL,
-					ARRAY_SIZE(rbspi_gpio_keys_reset),
-					rbspi_gpio_keys_reset);
-}
